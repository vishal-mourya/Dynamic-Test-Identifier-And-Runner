{
  "name": "ai-based-dynamic-test-identifier-runner",
  "version": "1.0.0",
  "description": "Chrome extension that analyzes PR code changes, identifies relevant tests, and triggers Jenkins pipelines automatically",
  "main": "background.js",
  "scripts": {
    "build": "node create-icons.js",
    "build:prod": "node scripts/build.js",
    "test": "node scripts/test.js",
    "test:unit": "echo \"Running unit tests...\"",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write src/**/*.js docs/**/*.md *.md",
    "zip": "zip -r ai-based-dynamic-test-runner.zip . -x node_modules/\\* .git/\\* legacy/\\* *.zip",
    "dev": "echo \"Load extension in Chrome Developer Mode from this directory\"",
    "validate": "node validate-manifest.js",
    "docs": "echo \"Documentation available in docs/ directory\"",
    "clean": "rm -rf dist/ *.zip",
    "watch": "echo \"Development mode - watching for changes\""
  },
  "keywords": [
    "chrome-extension",
    "testing",
    "jenkins",
    "ci-cd",
    "pull-request",
    "code-analysis",
    "test-automation",
    "github",
    "gitlab",
    "bitbucket"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vishal-mourya/ai-based-dynamic-test-identifier-runner.git"
  },
  "bugs": {
    "url": "https://github.com/vishal-mourya/ai-based-dynamic-test-identifier-runner/issues"
  },
  "homepage": "https://github.com/vishal-mourya/ai-based-dynamic-test-identifier-runner#readme",
  "devDependencies": {
    "eslint": "^8.0.0",
    "prettier": "^2.8.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "chrome": {
    "minimum_version": "88"
  }
}
